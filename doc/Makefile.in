bombard.1:
	@list='$(man_MANS)'; for p in $$list; do \
	if test -f $$p.in; then \
	  sed -e "s/%_DATE%/$(DATE)/"      \
	      -e "s/%_VERSION%/$(VERSION)/"  \
	      -e "s|%_PERL%|#!$(PERL)|"< $$p.in > $$p; \
	else :; fi; \
	done 

